{% extends 'base.html.twig' %}

{% block title %}Manufacturer{% endblock %}

{% block body %}
    <h1>Manufacturer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ manufacturer.id }}</td>
            </tr>
            <tr>
                <th>Fabricant</th>
                <td>{{ manufacturer.Fabricant }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ manufacturer.CreatedAt ? manufacturer.CreatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_manufacturer_index') }}">back to list</a>

    <a href="{{ path('app_manufacturer_edit', {'id': manufacturer.id}) }}">edit</a>

    {{ include('manufacturer/_delete_form.html.twig') }}
{% endblock %}
